{
  "rules": "firebase_rules.json",
  "hosting": {
    "public": "doc",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "snippets/**",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "https://dartpad.dev"
          }
        ]
      }
    ]
  }
}
